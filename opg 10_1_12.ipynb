   "source": [
    "def g(x):\n",
    "    return x**2 - 4\n",
    "\n",
    "def D_g(x):\n",
    "    return 2*x\n",
    "\n",
    "def f(x_n):\n",
    "    return x_n - g(x_n) / D_g(x_n)"
   ]
  },
