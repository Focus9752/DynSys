   ],
   "source": [
    "# Iterate f(x_n) starting from x_0=1\n",
    "\n",
    "x_0 = 1\n",
    "n = 10\n",
    "\n",
    "for i in range(n):\n",
    "    x_0 = f(x_0)\n",
    "    print(x_0)"
   ]
